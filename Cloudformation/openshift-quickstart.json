  "Outputs": {
    "ReleaseNote": {
      "Description": "Here is your Openshift Web Console!",
      "Value": {
        "Fn::Join": [
          "",
          [
            "https://",
            {
              "Fn::GetAtt": [
                "AdopEC2",
                "PublicIp"
              ]
            },
            ":8443/console"
          ]
        ]
      }
    },
    "SSH": {
      "Description": "Use this command to ssh into your ADOP EC2 instance.",
      "Value": {
        "Fn::Join": [
          "",
          [
            "ssh -i ",
            { "Ref": "KeyName" },
            ".pem ec2-user@",
            {
              "Fn::GetAtt": [
                "AdopEC2",
                "PublicIp"
              ]
            },
            ""
          ]
        ]
      }
    }
  },
  "Parameters": {
    "UseElasticIP": {
      "Type": "String",
      "Description": "Without Elastic IP, your EC2 instance public IP address will change everytime the system is restarted.",
      "AllowedValues": [ "true", "false" ],
      "ConstraintDescription": "Must be a boolean value.",
      "Default": "true"
    },
    "KeyName": {
      "Default": "",
      "Description": "SSH Key pair for accessing your EC2 instance",
      "Type": "AWS::EC2::KeyPair::KeyName"
    },
    "EC2VolumeSize": {
      "Type": "String",
      "Default": "50",
      "Description": "ADOP Core EC2 volume size in GB."
    },
    "EC2InstanceType": {
      "Type": "String",
      "Description": "ADOP Core EC2 HVM instance type.",
      "AllowedValues": [
        "t2.large",
        "m4.large",
        "m4.xlarge",
        "m3.large",
        "m3.xlarge"
      ],
      "ConstraintDescription": "Must be a valid EC2 HVM instance type.",
      "Default": "m4.large"
    }
  },
  "Conditions": {
    "UseElasticIPCondition" : {
      "Fn::Equals" : [ {"Ref" : "UseElasticIP"}, "true" ]
    }
  },
  "Resources": {
    "ElasticIP": {
      "Type": "AWS::EC2::EIP",
      "Condition": "UseElasticIPCondition",
      "Properties": {
        "InstanceId": {
          "Ref": "AdopEC2"
        }
      },
      "Metadata": {
        "AWS::CloudFormation::Designer": {
          "id": "77d8a590-a4b7-4657-ae49-2bace7b74166"
        }
      }
    },
    "AdopEC2": {
      "Properties": {
        "BlockDeviceMappings": [
          {
            "DeviceName": "/dev/xvda",
            "Ebs": {
              "DeleteOnTermination": "true",
              "VolumeSize": { "Ref": "EC2VolumeSize" },
              "VolumeType": "gp2"
            }
          }
        ],
        "NetworkInterfaces": [
          {
            "AssociatePublicIpAddress": {
						  "Fn::If" : [ "UseElasticIPCondition", "false", "true" ]
						},
            "DeleteOnTermination": "true",
            "DeviceIndex": "0",
            "GroupSet": [
              {
                "Ref": "AdopSecurityGroup"
              }
            ],
            "SubnetId": {
              "Ref": "PublicSubnet"
            }
          }
        ],
        "ImageId": {
          "Fn::FindInMap": [
            "RegionMap",
            {
              "Ref": "AWS::Region"
            },
            "CENTOSAMI"
          ]
        },
        "InstanceType": {
          "Ref": "EC2InstanceType"
        },
        "KeyName": {
          "Ref": "KeyName"
        },
        "Tags": [
          {
            "Key": "Name",
            "Value": {
              "Fn::Join": [
                "-",
                [
                  {
                    "Ref": "AWS::StackName"
                  },
                  "AdopEC2"
                ]
              ]
            }
          }
        ],
        "UserData": {
          "Fn::Base64": {
            "Fn::Join": [
              "",
              [
                "#!/bin/bash\n",
                "export IP=$(hostname --ip-address)\n",
                "yum -y install docker wget git\n",
                "sed -i \"s/#ADD_REGISTRY/ADD_REGISTRY/g\" /etc/sysconfig/docker\n",
                "echo \"INSECURE_REGISTRY='--insecure-registry 172.30.0.0/16'\" > /etc/sysconfig/docker\n",
                "systemctl start docker\n",
                "systemctl enable docker\n",
                "cd /opt\n",
                "wget https://github.com/openshift/origin/releases/download/v1.4.1/openshift-origin-client-tools-v1.4.1-3f9807a-linux-64bit.tar.gz\n",
                "tar -xvf openshift-origin-client-tools-v1.4.1-3f9807a-linux-64bit.tar.gz\n",
                "rm -fr *.tar.gz\n",
                "ln -sv $(pwd)/$(ls)/oc /usr/bin/oc\n",
                "export PUBLIC_HOSTNAME=$(curl http://169.254.169.254/latest/meta-data/public-hostname)\n",
                "export PUBLIC_IP=$(curl http://169.254.169.254/latest/meta-data/public-ipv4)\n",
                "oc cluster up --public-hostname=$PUBLIC_HOSTNAME --routing-suffix=${PUBLIC_IP}.xip.io --host-data-dir=$(pwd)/data --host-config-dir=$(pwd)/config --host-volumes-dir=$(pwd)/volumes\n",
                "oc login -u system:admin\n",
                "oc adm policy add-cluster-role-to-user cluster-admin admin --as=system:admin &> /dev/null\n",
                "curl -X PUT -H 'Content-Type:' --data-binary '{\"Status\" : \"SUCCESS\",",
                "\"Reason\" : \"ADOP is ready\",",
                "\"UniqueId\" : \"IDAdopCompletion\",",
                "\"Data\" : \"Done\"}' ",
                "\"",
                {
                  "Ref": "AdopWaitConditionHandle"
                },
                "\"\n",
                "\n"
              ]
            ]
          }
        }
      },
      "Type": "AWS::EC2::Instance",
      "Metadata": {
        "AWS::CloudFormation::Designer": {
          "id": "50a0d189-8c47-44b2-8eff-17759eb9c659"
        }
      }
    },
